{
	"name": "kcd-web-2023",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"format": "prettier --write --cache \"**/*.{js,jsx,ts,tsx,md,mdx}\"",
		"lint": "eslint src --ext js,jsx,mdx --ignore-path .gitignore --fix --cache --report-unused-disable-directives",
		"prepare": "lefthook install"
	},
	"dependencies": {
		"axios": "1.4.0",
		"firebase": "10.1.0",
		"lodash": "4.17.21",
		"svelte-fast-marquee": "0.4.1"
	},
	"devDependencies": {
		"@commitlint/cli": "17.6.5",
		"@commitlint/config-conventional": "17.6.5",
		"@neoconfetti/svelte": "1.0.0",
		"@skeletonlabs/skeleton": "1.11.0",
		"@sveltejs/adapter-static": "2.0.3",
		"@sveltejs/kit": "1.20.4",
		"@types/cookie": "0.5.1",
		"autoprefixer": "10.4.14",
		"eslint": "8.28.0",
		"eslint-plugin-svelte": "2.32.4",
		"lefthook": "1.4.3",
		"postcss": "8.4.27",
		"prettier": "2.8.0",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "4.0.5",
		"svelte-check": "3.4.3",
		"tailwind-merge": "1.14.0",
		"tailwindcss": "3.3.3",
		"tailwindcss-animate": "1.0.6",
		"vite": "4.4.2"
	},
	"type": "module",
	"pnpm": {
		"patchedDependencies": {
			"svelte-fast-marquee@0.4.1": "patches/svelte-fast-marquee@0.4.1.patch"
		}
	}
}
