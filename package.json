		"autoprefixer": "10.4.14",
		"eslint": "8.28.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte": "2.32.4",
		"lefthook": "1.4.3",
		"postcss": "8.4.27",
		"prettier": "2.8.0",
		"prettier-plugin-svelte": "2.10.1",
		"svelte": "4.0.5",
		"svelte-check": "3.4.3",
		"tailwindcss": "3.3.3",
